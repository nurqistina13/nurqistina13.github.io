# Define categories and corresponding keywords
categories <- c(
"Accountancy" = "accountancy",
"Engineering - Bioengineering" = "engineering - bioengineering",
"Education" = "education",
"Science" = c("science", "food", "chemistry", "sciences", "history", "physics", "culinary"),
"Arts" = c("communication studies", "art", "design & media", "chinese", "economics", "english", "linguistics", "philosophy"),
"Architecture" = "architecture",
"Business" = "business",
"Computing" = "computing",
"Dental" = "dental",
"Environmental Studies" = "environmental studies",
"Human Resource" = "human resource",
"Law" = c("law", "laws"),
"Medicine" = "medicine",
"Music" = "music",
"Real Estate" = "real estate",
"Social Sciences" = c("social sciences", "social science", "social work", "psychology", "Public Policy and Global Affairs", "sociology", "communication studies"),
"Information Systems" = "information systems",
"Maritime Studies" = "maritime studies"
)
# Create new datasets based on categories
new_datasets <- list()
for (category_name in names(categories)) {
keywords <- categories[[category_name]]
if (is.character(keywords)) {
keyword_data <- data[grepl(keywords, data$degree, ignore.case = TRUE), ]
} else {
keyword_data <- data[data$degree %in% unlist(keywords) & grepl(paste(keywords, collapse = "|"), data$degree, ignore.case = TRUE), ]
}
# Select relevant columns
selected_data <- keyword_data[, c("employment_rate_overall", "gross_monthly_mean")]
# Rename row names
rownames(selected_data) <- paste0(category_name, ".", 1:nrow(selected_data))
# Add to list of new datasets
new_datasets[[category_name]] <- selected_data
}
